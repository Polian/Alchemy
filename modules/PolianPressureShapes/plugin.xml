<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">

<plugin id="org.alchemy.create.PolianPressureShapes" version="1.0.1">
    
    <requires> 
        <import plugin-id="org.alchemy.core"/> 
    </requires> 
    
    <runtime>
        <library id="PolianPressureShapes" path="/" type="code">
            <export prefix="*" /> 
        </library>
        <library type="resources" path="data/" id="data" />
    </runtime>
    
    <extension plugin-id="org.alchemy.core" point-id="Create" id="PolianPressureShapesCreate"> 
        <parameter id="class" value="org.alchemy.create.PolianPressureShapes"/> 
        <parameter id="name" value="Polian Pressure Shapes"/> 
        <parameter id="description" value="Create shapes using pen pressure from a drawing tablet" />
        <parameter id="icon" value="icon.png" />
    </extension>
    
</plugin>